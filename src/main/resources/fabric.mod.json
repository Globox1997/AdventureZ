{
  "schemaVersion": 1,
  "id": "adventurez",
  "version": "${version}",
  "name": "AdventureZ",
  "description": "Adds deadly Creatures to Minecraft",
  "authors": [
    "Globox_Z"
  ],
  "contact": {
    "homepage": "https://www.curseforge.com/minecraft/mc-mods/adventurez",
    "sources": "https://github.com/Globox1997/AdventureZ",
    "issues": "https://github.com/Globox1997/AdventureZ/issues"
  },
  "license": "GPLv3",
  "icon": "assets/adventurez/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "net.adventurez.AdventureMain"
    ],
    "client": [
      "net.adventurez.AdventureClient"
    ],
    "modmenu": [
      "net.adventurez.config.ModMenuIntegration"
    ]
  },
  "mixins": [
    "adventurez.mixins.json"
  ],
  "depends": {
    "java": ">=17",
    "minecraft": "~${minecraft_version}",
    "fabric-api": ">=${fabric_version}",
    "fabricloader": ">=${loader_version}",
    "cloth-config2": ">=${cloth_config_version}"
  },
  "suggests": {
    "flamingo": "*"
  },
  "custom": {
    "mc-publish": {
      "modrinth": "defK2XM3",
      "curseforge": 390991
    }
